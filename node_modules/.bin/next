#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/g/stranka/mannheim/node_modules/.pnpm/next@12.2.3_7b277b0283d23f3e79ad0fe457158e91/node_modules/next/dist/bin/node_modules:/home/g/stranka/mannheim/node_modules/.pnpm/next@12.2.3_7b277b0283d23f3e79ad0fe457158e91/node_modules/next/dist/node_modules:/home/g/stranka/mannheim/node_modules/.pnpm/next@12.2.3_7b277b0283d23f3e79ad0fe457158e91/node_modules/next/node_modules:/home/g/stranka/mannheim/node_modules/.pnpm/next@12.2.3_7b277b0283d23f3e79ad0fe457158e91/node_modules:/home/g/stranka/mannheim/node_modules/.pnpm/node_modules:/home/g/stranka/mannheim/node_modules:/home/g/stranka/node_modules:/home/g/node_modules:/home/node_modules:/node_modules:/home/g/stranka/mannheim/node_modules/next/dist/bin/node_modules:/home/g/stranka/mannheim/node_modules/next/dist/node_modules:/home/g/stranka/mannheim/node_modules/next/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/g/stranka/mannheim/node_modules/.pnpm/next@12.2.3_7b277b0283d23f3e79ad0fe457158e91/node_modules/next/dist/bin/node_modules:/home/g/stranka/mannheim/node_modules/.pnpm/next@12.2.3_7b277b0283d23f3e79ad0fe457158e91/node_modules/next/dist/node_modules:/home/g/stranka/mannheim/node_modules/.pnpm/next@12.2.3_7b277b0283d23f3e79ad0fe457158e91/node_modules/next/node_modules:/home/g/stranka/mannheim/node_modules/.pnpm/next@12.2.3_7b277b0283d23f3e79ad0fe457158e91/node_modules:/home/g/stranka/mannheim/node_modules/.pnpm/node_modules:/home/g/stranka/mannheim/node_modules:/home/g/stranka/node_modules:/home/g/node_modules:/home/node_modules:/node_modules:/home/g/stranka/mannheim/node_modules/next/dist/bin/node_modules:/home/g/stranka/mannheim/node_modules/next/dist/node_modules:/home/g/stranka/mannheim/node_modules/next/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../next/dist/bin/next" "$@"
else
  exec node  "$basedir/../next/dist/bin/next" "$@"
fi
