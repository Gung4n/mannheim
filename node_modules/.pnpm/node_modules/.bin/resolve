#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/g/stranka/mannheim/my-app/node_modules/.pnpm/resolve@1.22.1/node_modules/resolve/bin/node_modules:/home/g/stranka/mannheim/my-app/node_modules/.pnpm/resolve@1.22.1/node_modules/resolve/node_modules:/home/g/stranka/mannheim/my-app/node_modules/.pnpm/resolve@1.22.1/node_modules:/home/g/stranka/mannheim/my-app/node_modules/.pnpm/node_modules:/home/g/stranka/mannheim/my-app/node_modules:/home/g/stranka/mannheim/node_modules:/home/g/stranka/node_modules:/home/g/node_modules:/home/node_modules:/node_modules:/home/g/stranka/mannheim/my-app/node_modules/.pnpm/node_modules/resolve/bin/node_modules:/home/g/stranka/mannheim/my-app/node_modules/.pnpm/node_modules/resolve/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/g/stranka/mannheim/my-app/node_modules/.pnpm/resolve@1.22.1/node_modules/resolve/bin/node_modules:/home/g/stranka/mannheim/my-app/node_modules/.pnpm/resolve@1.22.1/node_modules/resolve/node_modules:/home/g/stranka/mannheim/my-app/node_modules/.pnpm/resolve@1.22.1/node_modules:/home/g/stranka/mannheim/my-app/node_modules/.pnpm/node_modules:/home/g/stranka/mannheim/my-app/node_modules:/home/g/stranka/mannheim/node_modules:/home/g/stranka/node_modules:/home/g/node_modules:/home/node_modules:/node_modules:/home/g/stranka/mannheim/my-app/node_modules/.pnpm/node_modules/resolve/bin/node_modules:/home/g/stranka/mannheim/my-app/node_modules/.pnpm/node_modules/resolve/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../resolve/bin/resolve" "$@"
else
  exec node  "$basedir/../resolve/bin/resolve" "$@"
fi
