#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/g/stranka/mannheim/my-app/node_modules/.pnpm/typescript@4.7.4/node_modules/typescript/bin/node_modules:/home/g/stranka/mannheim/my-app/node_modules/.pnpm/typescript@4.7.4/node_modules/typescript/node_modules:/home/g/stranka/mannheim/my-app/node_modules/.pnpm/typescript@4.7.4/node_modules:/home/g/stranka/mannheim/my-app/node_modules/.pnpm/node_modules:/home/g/stranka/mannheim/my-app/node_modules:/home/g/stranka/mannheim/node_modules:/home/g/stranka/node_modules:/home/g/node_modules:/home/node_modules:/node_modules:/home/g/stranka/mannheim/my-app/node_modules/.pnpm/tsutils@3.21.0_typescript@4.7.4/node_modules/tsutils/node_modules:/home/g/stranka/mannheim/my-app/node_modules/.pnpm/tsutils@3.21.0_typescript@4.7.4/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/g/stranka/mannheim/my-app/node_modules/.pnpm/typescript@4.7.4/node_modules/typescript/bin/node_modules:/home/g/stranka/mannheim/my-app/node_modules/.pnpm/typescript@4.7.4/node_modules/typescript/node_modules:/home/g/stranka/mannheim/my-app/node_modules/.pnpm/typescript@4.7.4/node_modules:/home/g/stranka/mannheim/my-app/node_modules/.pnpm/node_modules:/home/g/stranka/mannheim/my-app/node_modules:/home/g/stranka/mannheim/node_modules:/home/g/stranka/node_modules:/home/g/node_modules:/home/node_modules:/node_modules:/home/g/stranka/mannheim/my-app/node_modules/.pnpm/tsutils@3.21.0_typescript@4.7.4/node_modules/tsutils/node_modules:/home/g/stranka/mannheim/my-app/node_modules/.pnpm/tsutils@3.21.0_typescript@4.7.4/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../typescript@4.7.4/node_modules/typescript/bin/tsc" "$@"
else
  exec node  "$basedir/../../../../../typescript@4.7.4/node_modules/typescript/bin/tsc" "$@"
fi
